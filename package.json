{
  "name": "automate-reports-api",
  "version": "1.0.0",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "npx tsx --env-file=.env --watch ./src/index.ts",
    "prettier": "npx prettier . --write",
    "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "lint": "npx eslint .",
    "postinstall": "patch-package",
    "db:createDb": "npx tsx ./src/sql-queries/createDb.ts",
    "db:createUser": "npx tsx ./src/sql-queries/createUser.ts",
    "db:changeUserPassword": "npx tsx ./src/sql-queries/changeUserPassword.ts",
    "db:changeUserName": "npx tsx ./src/sql-queries/changeUserName.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VladislavZgoda/automate-reports-api.git"
  },
  "author": "Zgoda Vladislav",
  "license": "MIT",
  "homepage": "https://github.com/VladislavZgoda/automate-reports-api#readme",
  "dependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "adm-zip": "^0.5.16",
    "bcrypt-ts": "^7.0.0",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "onchange": "^7.1.0",
    "patch-package": "^8.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^16.0.0",
    "jiti": "^2.4.2",
    "prettier": "^3.5.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  }
}
